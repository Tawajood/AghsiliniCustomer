<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/auth_nav.xml"
    app:startDestination="@id/splashFragment">

    <fragment
        android:id="@+id/splashFragment"
        android:name="com.dotjoo.aghsilinicustomer.ui.fragment.auth.splash.SplashFragment"
        android:label="SplashFragment" />
    <fragment
        android:id="@+id/walkThroughFragment"
        android:name="com.dotjoo.aghsilinicustomer.ui.fragment.auth.walkThrough.WalkThroughFragment"
        android:label="fragment_walk_through"
        tools:layout="@layout/fragment_walk_through" />
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.dotjoo.aghsilinicustomer.ui.fragment.auth.forget_password.login.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" />
    <fragment
        android:id="@+id/registerFragment"
        android:name="com.dotjoo.aghsilinicustomer.ui.fragment.auth.register.RegisterFragment"
        android:label="fragment_register"
        tools:layout="@layout/fragment_register" />
    <fragment
        android:id="@+id/forgetPasswordFragment"
        android:name="com.dotjoo.aghsilinicustomer.ui.fragment.auth.forget_password.ForgetPasswordFragment"
        android:label="ForgetPasswordFragment" />

    <fragment
        android:id="@+id/termsRegisterFragment"
        android:name="com.dotjoo.aghsilinicustomer.ui.fragment.auth.register.TermsRegisterFragment"
        android:label="TermsRegisterFragment" />
</navigation>